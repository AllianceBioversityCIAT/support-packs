{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mel-cop": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/mel-cop",
      "sourceRoot": "projects/mel-cop/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mel-cop",
            "index": "projects/mel-cop/src/index.html",
            "main": "projects/mel-cop/src/main.ts",
            "polyfills": "projects/mel-cop/src/polyfills.ts",
            "tsConfig": "projects/mel-cop/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/mel-cop/src/favicon.ico",
              "projects/mel-cop/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/mel-cop/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/mel-cop/src/environments/environment.ts",
                  "with": "projects/mel-cop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mel-cop:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "mel-cop:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mel-cop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mel-cop/src/test.ts",
            "polyfills": "projects/mel-cop/src/polyfills.ts",
            "tsConfig": "projects/mel-cop/tsconfig.spec.json",
            "karmaConfig": "projects/mel-cop/karma.conf.js",
            "assets": [
              "projects/mel-cop/src/favicon.ico",
              "projects/mel-cop/src/assets"
            ],
            "styles": [
              "projects/mel-cop/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/mel-cop/tsconfig.app.json",
              "projects/mel-cop/tsconfig.spec.json",
              "projects/mel-cop/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/mel-cop/e2e/protractor.conf.js",
            "devServerTarget": "mel-cop:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mel-cop:serve:production"
            }
          }
        }
      }
    },
    "termsconditions": {
      "projectType": "library",
      "root": "projects/termsconditions",
      "sourceRoot": "projects/termsconditions/src",
      "prefix": "tc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/termsconditions/tsconfig.lib.json",
            "project": "projects/termsconditions/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/termsconditions/src/test.ts",
            "tsConfig": "projects/termsconditions/tsconfig.spec.json",
            "karmaConfig": "projects/termsconditions/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/termsconditions/tsconfig.lib.json",
              "projects/termsconditions/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "mel-cop"
}