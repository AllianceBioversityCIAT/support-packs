{
  "name": "supportpacks-front",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --port=7000 --disable-host-check",
    "start-aiccrasp": "ng serve --project=aiccrasp --host=0.0.0.0 --port=6000 --disable-host-check",
    "start-dmsp": "ng serve --project=dmsp --host=0.0.0.0 --port=7000 --disable-host-check",
    "start-melsp": "ng serve --project=melsp --host=0.0.0.0 --port=8000 --disable-host-check",
    "start:dev": "forever start ./node_modules/@angular/cli/bin/ng serve --host=0.0.0.0 --port=7000 --disable-host-check",
    "build-melsp": "ng build --project=melsp --prod --deploy-url=/melsp/",
    "build-aiccrasp": "ng build --project=aiccrasp --prod --deploy-url=/aiccrasp/",
    "build-dmsp": "ng build --project=dmsp --prod --deploy-url=/dmsp/",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/**/src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ng-bootstrap/ng-bootstrap": "~5.2.2",
    "autosize": "^5.0.1",
    "bootstrap": "^4.5.2",
    "html2pdf.js": "^0.9.0",
    "install": "^0.13.0",
    "ngx-doc-viewer": "^1.4.1",
    "ngx-spinner": "^8.1.0",
    "rxjs": "~6.4.0",
    "sharepoint-rest": "^2.3.0",
    "sp-download": "^2.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "prettier": "^2.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
